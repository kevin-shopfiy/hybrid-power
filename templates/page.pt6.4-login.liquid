<div class="page-head pr tc{%- if settings.other_page_bg_image != blank %} page_jas_bg_image lazyload{%- endif -%}"{%- if settings.other_page_bg_image != blank %} data-bgset="{%- include 'bgset', image: settings.other_page_bg_image -%}" data-sizes="auto" data-parent-fit="cover" style="background-image: url({{ settings.other_page_bg_image | img_url: '50x50' }});"{%- endif -%}>
   <div class="jas-container pr">
      <h1 class="tu cw">{{ page.title }}</h1>
      {% comment %}<ul class="jas-breadcrumb dib oh">
         <li class="fl home"><a href="/" title="{{ 'general.breadcrumbs.home' | t }}">{{ 'general.breadcrumbs.home' | t }}</a></li>
         <li class="fl separator"> <i class="fa fa-angle-right"></i> </li>
         <li class="fl current"> {{page.title }}</li>
      </ul>{% endcomment %}
   </div>
</div>
<div class="jas-row jas-page">
   {%- if settings.use_map -%}
   <div class="jas-col-xs-12 jas-col-sm-12">
     <div class="gecko-gmap jas-gmap" data-lat='{{ settings.lat }}' data-lon='{{ settings.lon }}' {% if settings.icon_map != blank %}data-icon='{{ settings.icon_map | img_url: 'master' }}'{% endif %} data-style='{{ settings.style_json }}' style="height: {{ settings.height | strip }}px"></div>
   </div>
   {%- endif -%}
  <div class="jas-col-md-12 jas-col-xs-12 mt__60 mb__60"  role="main" itemscope="itemscope" itemtype="http://schema.org/CreativeWork">
      <div class="jas-container">
          <div class="login-can">
          {% if page.content.size > 0 %}<p>{{ page.content }}</p>{% endif %}
          <div class="jas-row mt_60 mb__60">
              <form id="idForm" action="" method="GET">
			       <label for="Can Gateway ID">Gateway ID </label> 
                   <input name="ID" id="ID" type="text"> <input type="submit" value="Visualize Data">
		      </form>
          </div>
          </div>
          <div class="jas-row mt_60 mb__60">                                                                                                    
          		<div id="loading-image" ><img src="{{ 'ajax-loader.gif' | asset_url }}"></div>
          </div>
          <div id="can-data"></div>
      	  <script type="text/javascript" src="{{ 'can-api.js' | asset_url }}"></script>
      </div>                                                                                                
  </div>
</div>
